{
	"build": [
		"rm -rf vendor",
		"composer install --no-dev -o",
		"bun run build"
	],
	"build_dev": [
		"rm -rf vendor",
		"composer install -o",
		"bun run build"
	],
	"paths": {
		"versions": [
			{
				"file": "plugin.php",
				"regex": "\\* (Version: +)(.+)"
			}
		]
	},
	"repo_url": "https://github.com/stellarwp/stellar-changelog-embed",
	"zip_name": "stellar-changelog-embed"
}
